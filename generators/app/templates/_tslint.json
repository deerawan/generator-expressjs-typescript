{
  "extends": "tslint:latest",
  "rules": {
    "indent": [true, 2],
    "interface-name": [true, "never-prefix"],
    "max-line-length": [true, 150],
    "quotemark": [true, "single"],
    "object-literal-sort-keys": false,
    "no-var-requires": false,
    "ordered-imports": false,
    "only-arrow-functions": false
  }
}
